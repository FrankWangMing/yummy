datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  // previewFeatures = []
}

generator dbml {
  provider = "prisma-dbml-generator"
}

model User {
  id     Int     @id @default(autoincrement())
  email  String  @unique
  name   String?
  Room   Room?   @relation(fields: [roomId], references: [id])
  roomId Int?
}

model Room {
  id    Int    @id @default(autoincrement())
  users User[]
}
